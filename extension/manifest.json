
{
    "name": "LinkBack",
    "version": "2.0",
    "manifest_version": 2,
    "description": "Extension to display incoming links.",
    "browser_action": {
	"default_icon": "mirror.jpg"
    },
    "permissions": [
	"webRequest","<all_urls>"
    ],
    "content_scripts": [
	{
	    "matches": ["<all_urls>"],
	    "js": ["script.js"]
	}
    ]

}
